color: #dfe;\n    color: #041;\n\n    .dark & {\n      background-color: #021;\n      color: #ced;\n    }\n  }\n\n  &.warning {\n    border-color: #ea0;\n    background-color: #fff7f0;\n    color: #960;\n\n    .dark & {\n      background-color: #210;\n      color: #ea0;\n    }\n  }\n\n  &.danger {\n    border-color: #900;\n    background-color: #fee;\n    color: #600;\n\n    .dark & {\n      background-color: #300;\n      color: #fcc;\n    }\n  }\n\n  a {\n    font-weight: inherit !important;\n    text-decoration-style: dotted;\n    text-decoration: underline;\n  }\n}\n\n// Details Blocks\ndetails.custom-block.details {\n  padding: 11px 14px 1px;\n\n  .dark & {\n    background-color: #222;\n  }\n\n  summary {\n    font-weight: 500;\n    font-style: italic;\n\n    a {\n      color: darkcyan\n    }\n  }\n}\n\nimg {\n  margin: 30px auto;\n}\n\n.good {\n  color: teal\n}\n\n.bad,\n.important {\n  color: darkred\n}\n\n;\n\n.dark & {\n\n  .bad,\n  .important {\n    color: #e00\n  }\n}\n\n.constructor::before {\n  content: 'Constructor: ';\n  color: grey\n}\n\n.property::before {\n  content: 'Property: ';\n  color: grey\n}\n\n.method::before {\n  content: 'Method: ';\n  color: grey\n}\n\n.event::before {\n  content: 'Event: ';\n  color: grey\n}\n\n.class::before {\n  content: 'Class: ';\n  color: grey\n}\n\n.async-method::before {\n  content: 'Method: async ';\n  color: grey\n}\n\nh3.event,\nh3.method,\nh3.async-method {\n  margin-top: 5em;\n  // font-size: 22px;\n}\n\nh4.property+h4.property,\nh4.event+h4.event,\nh4.method+h4.method,\nh3.property+h3.property,\nh3.event+h3.event,\nh3.class+h3.class,\nh3.method+h3.method {\n  margin-top: 0\n}\n\na[target=\"_blank\"]:not(.no-ext-link) {\n  &::after {\n    font-size: 0.7em;\n    content: \"\\00A0\\2197\";\n  }\n}\n\n\n// \"On this page\" Outlines\ndiv.aside nav {\n  ul {\n    font-size: 15px !important;\n\n    ul.nested {\n      font-size: 95% !important;\n      padding: 7px 0 7px 20px;\n\n      li a {\n        line-height: 2em !important;\n      }\n    }\n  }\n}\n\n// Hide empty table headers\ntr:has(> th:empty:first-child:last-child),\ntr:has(> th:empty:first-child:not(:last-child) + th:empty:last-child) {\n  display: none;\n}\n\nkbd {\n  font-size: 12px;\n  display: inline-block;\n  padding: 5px;\n  line-height: 10px;\n  vertical-align: middle;\n  border: 1px solid var(--vp-c-divider);\n  border-radius: 6px;\n  box-shadow: inset 0 -1px 0 var(--vp-c-divider);\n  position: relative;\n  top: -1px;\n}\n\n.learn-more {\n  color: var(--vp-c-gray-1);\n  font-style: italic;\n  margin-top: -5px;\n  margin-bottom: 5px;\n  display: block;\n}\n\n.learn-more::before {\n  content: url('https://cap.cloud.sap/docs/images/learn-more.svg');\n  vertical-align: middle;\n  height: 20px;\n  width: 20px;\n  display: inline-block;\n  margin-right: 5px;\n}\n\n/* remove transparency in the odd rows (when it scrolls over outline) */\ntr:nth-child(odd) {\n  background-color: var(--vp-c-bg);\n}\n\n\n/* expand width on big screens */\n@media screen and (min-width: 1600px) {\n\n  div[class^='language-']:hover,\n  div[class^='language-']:focus,\n  pre.log:hover,\n  pre.log:focus {\n    min-width: fit-content;\n    padding-right: 40px;\n  }\n\n  table:hover,\n  table:focus {\n    min-width: fit-content;\n  }\n\n  [class*='language-'] pre {\n    overflow: hidden !important;\n  }\n}\n</style>\n\n<html lang=\"en-US\" dir=\"ltr\"><body><div id=\"app\"><div class=\"Layout\" data-v-9ae704ff=\"\" data-v-d8b57b2d=\"\"><div class=\"VPContent has-sidebar\" id=\"VPContent\" data-v-d8b57b2d=\"\" data-v-9a6c75ad=\"\"><div class=\"VPDoc has-sidebar has-aside\" data-v-9a6c75ad=\"\" data-v-e6f2a212=\"\"><div class=\"container\" data-v-e6f2a212=\"\"><div class=\"content\" data-v-e6f2a212=\"\"><div class=\"content-container\" data-v-e6f2a212=\"\"><main class=\"main\" data-v-e6f2a212=\"\"><div style=\"position:relative;\" class=\"vp-doc _docs_get-started_in-a-nutshell external-link-icon-enabled\" data-v-e6f2a212=\"\"><div data-v-e4c3dca8=\"\"><div class=\"impl node\" data-v-e4c3dca8=\"\"><p data-v-e4c3dca8=\"\"><a href=\"https://cap.cloud.sap/docs/node.js/core-services%23srv-on-before-after\" class=\"learn-more\" data-v-e4c3dca8=\"\">Learn more about adding <strong data-v-e4c3dca8=\"\">event handlers</strong> using <code data-v-e4c3dca8=\"\"><srv>.on/before/after</code>.</a></p></div><h3 id=\"consuming-other-services\" tabindex=\"-1\" data-v-e4c3dca8=\"\">Consuming Other Services <a class=\"header-anchor\" href=\"#consuming-other-services\" aria-label=\"Permalink to \"Consuming Other Services\"\" data-v-e4c3dca8=\"\">​</a></h3><p data-v-e4c3dca8=\"\">Quite frequently, event handler implementations consume other services, sending requests and queries, as in the completed example below.</p></div></div></main></div></div></div></div></div></div></div></body></html>",
